{
    "name": "emteknetz/octopus",
    "type": "silverstripe-vendormodule",
    "description": "POC for GitHub actions CI",
    "require": {
        "silverstripe/framework": "^4"
    },
    "require-dev": {
        "sminnee/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "emteknetnz\\Octopus\\": "src/",
            "emteknetnz\\Octopus\\Tests\\": "tests/",
            "emteknetnz\\Octopus\\Tests\\Behat\\Context\\": "tests/behat/src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
